<%- include('includes/head.ejs') %>
    <link rel="stylesheet" href="/css/card.css">
</head>
<body>
    <%- include('includes/navigation.ejs') %>
    <main>
        <div class="card">
            <div class="card-text-wrapper">
                <% if(users.length > 0)  { %>
                    <% for(const user of users) { %>
                        <h1><%= user.name %></h1>
                    <% } %>
                <% } else { %>
                    <h1>No User</h1>
                <% } %>
            </div>
        </div>
    </main>
<%- include('includes/end.ejs') %>